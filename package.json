    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.8",
    "lint-staged": "^13.2.3",
    "node-mocks-http": "^1.12.1",
    "prettier": "^2.8.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  }
}
