    "@nestjs/swagger": "^7.0.2",
    "@nestjs/typeorm": "^10.0.0",
    "async": "^3.2.4",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "fcm-node": "^1.6.1",
    "i": "^0.3.7",
    "install": "^0.13.0",
    "mysql2": "^3.0.1",
    "npm": "^9.4.0",
    "swagger-ui-express": "^4.1.6",
    "tsd": "^0.22.0",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
